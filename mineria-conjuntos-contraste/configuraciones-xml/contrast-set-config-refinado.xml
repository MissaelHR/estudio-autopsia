<?xml version="1.0"?>
<experiment>
  <parameter_sets>
    <parameter_set name="Refinado">
      <param name="induction_measure">{{MEASURE}}</param>
      <param name="pruning_measure">{{MEASURE}}</param>
      <param name="voting_measure">{{MEASURE}}</param>
		  <param name="minsupp_all">0.05</param>
		  <param name="minsupp_new">0.05</param>
		  <param name="max_neg2pos">2</param>
		  <param name="max_passes_count">{{MAX_PASSES_COUNT}}</param>
		  <param name="penalty_strength">0.5</param>
		  <param name="penalty_saturation">0.2</param>
    </parameter_set>
  </parameter_sets>

  <datasets>
    <dataset>
      <label>centro_hospitalario</label>
      <contrast_attribute>centro_hospitalario</contrast_attribute>
      <out_directory>./output/</out_directory>
      <training>
        <report_file>{{REPORT_FILE_NAME}}</report_file>
        <train>
          <in_file>{{DATASET_FILE_NAME_ARFF}}</in_file>
          <model_file>{{DATASET_FILE_NAME_MDL}}</model_file>
          <model_csv>{{DATASET_FILE_NAME_CSV}}</model_csv>
        </train>
      </training>
    </dataset>
  </datasets>
</experiment>
