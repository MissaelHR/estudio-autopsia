		  <param name="penalty_saturation">0.2</param>
      <param name="max_rule_count">{{MAX_RULE_COUNT}}</param>
    </parameter_set>

    <parameter_set name="Correlation">
      <param name="induction_measure">Correlation</param>
      <param name="pruning_measure">Correlation</param>
      <param name="voting_measure">Correlation</param>
		  <param name="minsupp_all">0.07</param>
		  <param name="minsupp_new">0.07</param>
		  <param name="max_neg2pos">2</param>
		  <param name="max_passes_count">{{MAX_PASSES_COUNT}}</param>
		  <param name="penalty_strength">0.5</param>
		  <param name="penalty_saturation">0.2</param>
      <param name="max_rule_count">{{MAX_RULE_COUNT}}</param>
    </parameter_set>

    <parameter_set name="Klosgen">
      <param name="induction_measure">Klosgen</param>
