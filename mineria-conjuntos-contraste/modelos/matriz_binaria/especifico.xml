		  <param name="max_passes_count">{{MAX_PASSES_COUNT}}</param>
		  <param name="penalty_strength">0.5</param>
		  <param name="penalty_saturation">0.2</param>
      <param name="max_rule_count">{{MAX_RULE_COUNT}}</param>
    </parameter_set>

    <parameter_set name="RelativeRisk">
      <param name="induction_measure">RelativeRisk</param>
      <param name="pruning_measure">RelativeRisk</param>
      <param name="voting_measure">RelativeRisk</param>
		  <param name="minsupp_all">0.07</param>
		  <param name="minsupp_new">0.07</param>
		  <param name="max_neg2pos">2</param>
		  <param name="max_passes_count">{{MAX_PASSES_COUNT}}</param>
		  <param name="penalty_strength">0.5</param>
		  <param name="penalty_saturation">0.2</param>
      <param name="max_rule_count">{{MAX_RULE_COUNT}}</param>
    </parameter_set>

  </parameter_sets>
  </parameter_sets>

  <datasets>
    <dataset>
      <label>centro_hospitalario</label>
      <contrast_attribute>centro_hospitalario</contrast_attribute>
      <out_directory>./resultados/matriz_binaria/especifico/</out_directory>
      <training>
        <report_file>Resultados_Matriz_Binaria</report_file>
        <train>
          <in_file>matriz_binaria.arff</in_file>
          <model_file>matriz_binaria.mdl</model_file>
          <model_csv>matriz_binaria.csv</model_csv>
        </train>
      </training>
    </dataset>
  </datasets>
</experiment>